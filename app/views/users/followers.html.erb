<div class="container">
  <h2 class="mt-2" style="color:white"><%= current_user.nickname %></h2>
  </br>
  <% @followers.each do |follows| %>
    <p>
      <%= follows.nickname %>
      <%= link_to "Start Conversation", chatrooms_path(recipient_id: follows.id), method: :post, class: "btn btn-success" %>
    </p>
  <% end %>
</div>
